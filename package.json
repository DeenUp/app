{
	"name": "deenup",
	"private": true,
	"main": "expo-router/entry",
	"version": "0.0.1",
	"packageManager": "pnpm@9.1.2",
	"scripts": {
		"inc-version": "node ./scripts/increment-version && git add version.json",
		"prepare-env": "cp .env.$FLAVOR .env",
		"_start": "pnpm run prepare-env && npx expo start -c",
		"start:dev": "FLAVOR=dev pnpm run _start",
		"start:prod": "FLAVOR=production pnpm run _start",
		"start:stg": "FLAVOR=staging pnpm run _start",
		"_build": "node ./scripts/build",
		"eas-build-pre-install": "./scripts/pre-build",
		"build:ios:dev": "FLAVOR=dev PLATFORM=ios pnpm run _build",
		"build:ios:prod": "FLAVOR=production PLATFORM=ios pnpm run _build",
		"build:ios:stg": "FLAVOR=staging PLATFORM=ios pnpm run _build",
		"build:android:dev": "FLAVOR=dev PLATFORM=android pnpm run _build",
		"build:android:prod": "FLAVOR=production PLATFORM=android pnpm run _build",
		"build:android:stg": "FLAVOR=staging PLATFORM=android pnpm run _build",
		"build:web:dev": "FLAVOR=dev PLATFORM=web pnpm run _build",
		"build:web:prod": "FLAVOR=production PLATFORM=web pnpm run _build",
		"build:web:stg": "FLAVOR=staging PLATFORM=web pnpm run _build",
		"run:android:dev": "FLAVOR=dev pnpm run _run:android",
		"run:ios:dev": "FLAVOR=dev pnpm run _run:ios",
		"run:ios:staging": "FLAVOR=staging pnpm run _run:ios",
		"run:ios:production": "FLAVOR=production pnpm run _run:ios",
		"_run:android": "pnpm run prepare-env && expo run:android",
		"_run:ios": "pnpm run prepare-env && expo run:ios",
		"test": "jest --watchAll",
		"test:e2e:android": "detox test --configuration android.emu.debug",
		"test:e2e:ios": "detox test --configuration ios.sim.debug",
		"build:e2e:android": "detox build --configuration android.emu.debug",
		"build:e2e:ios": "detox build --configuration ios.sim.debug",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"typecheck": "tsc --noEmit",
		"prepare": "husky install",
		"android": "expo run:android",
		"ios": "expo run:ios"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
		]
	},
	"dependencies": {
		"@aws-amplify/react-native": "^1.1.1",
		"@expo/metro-config": "^0.17.7",
		"@expo/vector-icons": "^14.0.2",
		"@react-native-async-storage/async-storage": "1.21.0",
		"@react-native-community/netinfo": "^11.3.2",
		"aws-amplify": "^6.3.4",
		"expo": "~50.0.19",
		"expo-build-properties": "~0.11.1",
		"expo-clipboard": "^5.0.1",
		"expo-constants": "~15.4.6",
		"expo-dev-client": "~3.3.12",
		"expo-font": "^11.10.3",
		"expo-image-picker": "~14.7.1",
		"expo-linking": "~6.2.2",
		"expo-localization": "~14.8.4",
		"expo-router": "~3.4.10",
		"expo-splash-screen": "~0.26.5",
		"expo-status-bar": "~1.11.1",
		"expo-system-ui": "~2.9.4",
		"i18n-js": "^4.4.3",
		"lottie-react-native": "6.5.1",
		"moti": "^0.28.1",
		"nativewind": "4.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "~0.73.6",
		"react-native-css-interop": "^0.0.13",
		"react-native-gesture-handler": "~2.14.1",
		"react-native-get-random-values": "^1.11.0",
		"react-native-paper": "^5.12.3",
		"react-native-reanimated": "~3.6.3",
		"react-native-safe-area-context": "~4.8.2",
		"react-native-screens": "~3.29.0",
		"react-native-vector-icons": "^10.1.0",
		"react-native-web": "~0.19.12",
		"rxjs": "^7.8.1",
		"superjson": "2.2.1",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"ts-jest": "^29.1.3",
		"twrnc": "^4.2.0",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.24.6",
		"@babel/preset-env": "^7.24.6",
		"@babel/runtime": "^7.24.6",
		"@eslint/js": "^9.3.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.2.1",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/babel__core": "^7.20.5",
		"@types/eslint": "^8.56.10",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.2.79",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.10.0",
		"@typescript-eslint/parser": "^7.10.0",
		"detox": "^20.22.2",
		"enforce-branch-name": "^1.1.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-auto-import": "^0.1.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-plugin-sort-exports": "^0.9.1",
		"eslint-plugin-tailwindcss": "^3.17.0",
		"eslint-plugin-unused-imports": "^3.2.0",
		"globals": "^15.3.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"jest-expo": "~50.0.4",
		"lint-staged": "^15.2.5",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"react-test-renderer": "18.2.0",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.10.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.*": [
			"pnpm format:fix"
		]
	}
}
